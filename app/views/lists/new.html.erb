<h1>New list</h1>

  <%= simple_form_for(@list) do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :image_url %>
</div>

<div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
  <% end %>

<br>

<div>
  <%= link_to "Back to lists", lists_path, class: 'btn btn-success' %>
</div>
